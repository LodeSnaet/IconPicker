{
  "name": "lode-snaet/craft-icon-picker",
  "description": "This plugin lets you configure a fontawesome kit.",
  "type": "craft-plugin",
  "license": "proprietary",
  "support": {
    "email": "lode.snaet@outlook.com",
    "issues": "https://github.com/lodesnaet/iconpicker.git/issues?state=open",
    "source": "https://github.com/lodesnaet/iconpicker.git",
    "docs": "https://github.com/lodesnaet/iconpicker.git",
    "rss": "https://github.com/lodesnaet/iconpicker.git/releases.atom"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.8.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "lodesnaet\\crafticonpicker\\": "src/"
    }
  },
  "extra": {
    "handle": "icon-picker",
    "name": "IconPicker",
    "developer": "LodeSnaet",
    "documentationUrl": "https://github.com/lodesnaet/iconpicker.git",
    "class": "lodesnaet\\crafticonpicker\\IconPicker"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}